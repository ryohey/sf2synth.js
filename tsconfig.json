{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2016", "dom"],
    "sourceMap": true,
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
